# =============================================================================
# OBJECT PROPERTIES (Relationships)
# =============================================================================

# Company Relationships
sei-co:hasSubsidiary rdf:type owl:ObjectProperty ;
    rdfs:domain sei-co:Company ;
    rdfs:range sei-co:Subsidiary ;
    rdfs:label "has subsidiary"@en ;
    owl:inverseOf sei-co:isSubsidiaryOf .

sei-co:isSubsidiaryOf rdf:type owl:ObjectProperty ;
    rdfs:domain sei-co:Subsidiary ;
    rdfs:range sei-co:Company ;
    rdfs:label "is subsidiary of"@en .

sei-co:competesWith rdf:type owl:ObjectProperty ;
    rdf:type owl:SymmetricProperty ;
    rdfs:domain sei-co:Company ;
    rdfs:range sei-co:Company ;
    rdfs:label "competes with"@en .

sei-co:partnersWith rdf:type owl:ObjectProperty ;
    rdf:type owl:SymmetricProperty ;
    rdfs:domain sei-co:Company ;
    rdfs:range sei-co:Company ;
    rdfs:label "partners with"@en .

sei-co:acquired rdf:type owl:ObjectProperty ;
    rdfs:domain sei-co:Company ;
    rdfs:range sei-co:Company ;
    rdfs:label "acquired"@en .

sei-co:suppliesTo rdf:type owl:ObjectProperty ;
    rdfs:domain sei-co:Supplier ;
    rdfs:range sei-co:Company ;
    rdfs:label "supplies to"@en .

sei-co:belongsToSector rdf:type owl:ObjectProperty ;
    rdfs:domain sei-co:Company ;
    rdfs:range sei-co:IndustrySector ;
    rdfs:label "belongs to sector"@en .

# Financial Relationships
sei-fin:hasFinancialMetric rdf:type owl:ObjectProperty ;
    rdfs:domain sei-co:Company ;
    rdfs:range sei-fin:FinancialMetric ;
    rdfs:label "has financial metric"@en .

sei-fin:reportedIn rdf:type owl:ObjectProperty ;
    rdfs:domain sei-fin:FinancialMetric ;
    rdfs:range sei-doc:Document ;
    rdfs:label "reported in"@en .

sei-fin:hasSecurity rdf:type owl:ObjectProperty ;
    rdfs:domain sei-co:PublicCompany ;
    rdfs:range sei-fin:Security ;
    rdfs:label "has security"@en .

# Document Relationships
sei-doc:filedBy rdf:type owl:ObjectProperty ;
    rdfs:domain sei-doc:SECFiling ;
    rdfs:range sei-co:PublicCompany ;
    rdfs:label "filed by"@en .

sei-doc:hasSection rdf:type owl:ObjectProperty ;
    rdfs:domain sei-doc:Document ;
    rdfs:range sei-doc:DocumentSection ;
    rdfs:label "has section"@en .

sei-doc:mentionsEntity rdf:type owl:ObjectProperty ;
    rdfs:domain sei-doc:Document ;
    rdfs:range owl:Thing ;
    rdfs:label "mentions entity"@en .

sei-doc:containsMetric rdf:type owl:ObjectProperty ;
    rdfs:domain sei-doc:Document ;
    rdfs:range sei-fin:FinancialMetric ;
    rdfs:label "contains metric"@en .

# Risk Relationships
sei-risk:facesRisk rdf:type owl:ObjectProperty ;
    rdfs:domain sei-co:Company ;
    rdfs:range sei-risk:Risk ;
    rdfs:label "faces risk"@en .

sei-risk:disclosedIn rdf:type owl:ObjectProperty ;
    rdfs:domain sei-risk:Risk ;
    rdfs:range sei-doc:Document ;
    rdfs:label "disclosed in"@en .

sei-risk:affectsMetric rdf:type owl:ObjectProperty ;
    rdfs:domain sei-risk:Risk ;
    rdfs:range sei-fin:FinancialMetric ;
    rdfs:label "affects metric"@en .

# Economic Relationships
sei-econ:impactedBy rdf:type owl:ObjectProperty ;
    rdfs:domain sei-co:Company ;
    rdfs:range sei-econ:EconomicIndicator ;
    rdfs:label "impacted by"@en .

sei-econ:correlatesWith rdf:type owl:ObjectProperty ;
    rdf:type owl:SymmetricProperty ;
    rdfs:domain sei-econ:EconomicIndicator ;
    rdfs:range sei-fin:FinancialMetric ;
    rdfs:label "correlates with"@en .

