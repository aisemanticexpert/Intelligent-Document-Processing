sei-risk:CompetitiveRisk rdf:type owl:Class ;
    rdfs:subClassOf sei-risk:Risk ;
    rdfs:label "Competitive Risk"@en .

sei-risk:TechnologyRisk rdf:type owl:Class ;
    rdfs:subClassOf sei-risk:Risk ;
