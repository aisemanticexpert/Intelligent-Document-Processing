# =============================================================================
# ONTOLOGY DECLARATION
# =============================================================================

<http://www.semanticexpert.ai/ontology/financial>
    rdf:type owl:Ontology ;
    owl:versionIRI <http://www.semanticexpert.ai/ontology/financial/1.0.0> ;
    dcterms:title "Semantic Expert Financial IDR Ontology"@en ;
    dcterms:description "A comprehensive ontology for Intelligent Document Recognition in the financial domain, supporting 10-K/10-Q filings, macroeconomic data, and company semantic services."@en ;
    dcterms:creator "Rajesh Kumar Gupta" ;
    dcterms:created "2024-12-01"^^xsd:date ;
    owl:imports <https://spec.edmcouncil.org/fibo/ontology/FND/Organizations/Organizations/> .

# =============================================================================
# COMPANY CLASSES (sei-co namespace)
# =============================================================================

sei-co:Organization rdf:type owl:Class ;
    rdfs:label "Organization"@en ;
    rdfs:comment "A legal entity or group of individuals organized for a purpose."@en ;
    skos:definition "The base class for all organizational entities."@en .

sei-co:Company rdf:type owl:Class ;
    rdfs:subClassOf sei-co:Organization ;
    rdfs:label "Company"@en ;
    rdfs:comment "A business entity engaged in commercial, industrial, or professional activities."@en ;
    skos:example "Apple Inc., Microsoft Corporation"@en .

sei-co:PublicCompany rdf:type owl:Class ;
    rdfs:subClassOf sei-co:Company ;
    rdfs:label "Public Company"@en ;
    rdfs:comment "A company whose shares are traded on a public stock exchange."@en .

sei-co:PrivateCompany rdf:type owl:Class ;
    rdfs:subClassOf sei-co:Company ;
    rdfs:label "Private Company"@en ;
    rdfs:comment "A company not listed on public exchanges."@en .

sei-co:Subsidiary rdf:type owl:Class ;
    rdfs:subClassOf sei-co:Company ;
    rdfs:label "Subsidiary"@en ;
    rdfs:comment "A company controlled by another company (parent)."@en .

sei-co:Supplier rdf:type owl:Class ;
    rdfs:subClassOf sei-co:Organization ;
    rdfs:label "Supplier"@en ;
    rdfs:comment "An organization that provides goods or services."@en .

sei-co:Competitor rdf:type owl:Class ;
    rdfs:subClassOf sei-co:Company ;
    rdfs:label "Competitor"@en ;
    rdfs:comment "A company competing in the same market."@en .

# Industry Sector Classes
sei-co:IndustrySector rdf:type owl:Class ;
    rdfs:label "Industry Sector"@en ;
    rdfs:comment "A classification of business activities."@en .

sei-co:TechnologySector rdf:type owl:Class ;
    rdfs:subClassOf sei-co:IndustrySector ;
    rdfs:label "Technology Sector"@en .

sei-co:HealthcareSector rdf:type owl:Class ;
    rdfs:subClassOf sei-co:IndustrySector ;
    rdfs:label "Healthcare Sector"@en .

sei-co:FinancialSector rdf:type owl:Class ;
    rdfs:subClassOf sei-co:IndustrySector ;
    rdfs:label "Financial Services Sector"@en .

sei-co:ConsumerSector rdf:type owl:Class ;
    rdfs:subClassOf sei-co:IndustrySector ;
    rdfs:label "Consumer Goods Sector"@en .

sei-co:EnergySector rdf:type owl:Class ;
    rdfs:subClassOf sei-co:IndustrySector ;
    rdfs:label "Energy Sector"@en .

sei-co:IndustrialsSector rdf:type owl:Class ;
    rdfs:subClassOf sei-co:IndustrySector ;
    rdfs:label "Industrials Sector"@en .

# =============================================================================
# FINANCIAL CLASSES (sei-fin namespace)
# =============================================================================

sei-fin:FinancialInstrument rdf:type owl:Class ;
    rdfs:label "Financial Instrument"@en ;
    rdfs:comment "A tradeable asset or contract."@en .

sei-fin:Security rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:FinancialInstrument ;
    rdfs:label "Security"@en ;
    rdfs:comment "A tradeable financial asset."@en .

sei-fin:Equity rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:Security ;
    rdfs:label "Equity"@en ;
    rdfs:comment "Ownership interest in a company."@en .

sei-fin:CommonStock rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:Equity ;
    rdfs:label "Common Stock"@en .

sei-fin:PreferredStock rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:Equity ;
    rdfs:label "Preferred Stock"@en .

sei-fin:Bond rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:Security ;
    rdfs:label "Bond"@en ;
    rdfs:comment "A debt security."@en .

sei-fin:Derivative rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:FinancialInstrument ;
    rdfs:label "Derivative"@en ;
    rdfs:comment "A contract whose value derives from an underlying asset."@en .

# Financial Metrics Classes
sei-fin:FinancialMetric rdf:type owl:Class ;
    rdfs:label "Financial Metric"@en ;
    rdfs:comment "A quantitative measure of financial performance."@en .

sei-fin:Revenue rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:FinancialMetric ;
    rdfs:label "Revenue"@en ;
    skos:altLabel "Sales", "Net Sales", "Total Revenue"@en .

sei-fin:NetIncome rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:FinancialMetric ;
    rdfs:label "Net Income"@en ;
    skos:altLabel "Profit", "Earnings", "Net Profit"@en .

sei-fin:GrossProfit rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:FinancialMetric ;
    rdfs:label "Gross Profit"@en .

sei-fin:OperatingIncome rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:FinancialMetric ;
    rdfs:label "Operating Income"@en ;
    skos:altLabel "EBIT", "Operating Profit"@en .

sei-fin:EBITDA rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:FinancialMetric ;
    rdfs:label "EBITDA"@en ;
    rdfs:comment "Earnings Before Interest, Taxes, Depreciation and Amortization."@en .

sei-fin:EarningsPerShare rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:FinancialMetric ;
    rdfs:label "Earnings Per Share"@en ;
    skos:altLabel "EPS"@en .

sei-fin:TotalAssets rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:FinancialMetric ;
    rdfs:label "Total Assets"@en .

sei-fin:TotalLiabilities rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:FinancialMetric ;
    rdfs:label "Total Liabilities"@en .

sei-fin:ShareholdersEquity rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:FinancialMetric ;
    rdfs:label "Shareholders Equity"@en ;
    skos:altLabel "Stockholders Equity", "Book Value"@en .

sei-fin:CashFlow rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:FinancialMetric ;
    rdfs:label "Cash Flow"@en .

sei-fin:OperatingCashFlow rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:CashFlow ;
    rdfs:label "Operating Cash Flow"@en .

sei-fin:FreeCashFlow rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:CashFlow ;
    rdfs:label "Free Cash Flow"@en .

# Financial Ratios
sei-fin:FinancialRatio rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:FinancialMetric ;
    rdfs:label "Financial Ratio"@en .

sei-fin:PriceToEarnings rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:FinancialRatio ;
    rdfs:label "Price to Earnings Ratio"@en ;
    skos:altLabel "P/E Ratio"@en .

sei-fin:DebtToEquity rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:FinancialRatio ;
    rdfs:label "Debt to Equity Ratio"@en .

sei-fin:ReturnOnEquity rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:FinancialRatio ;
    rdfs:label "Return on Equity"@en ;
    skos:altLabel "ROE"@en .

sei-fin:ReturnOnAssets rdf:type owl:Class ;
    rdfs:subClassOf sei-fin:FinancialRatio ;
    rdfs:label "Return on Assets"@en ;
    skos:altLabel "ROA"@en .

# =============================================================================
# DOCUMENT CLASSES (sei-doc namespace)
# =============================================================================

sei-doc:Document rdf:type owl:Class ;
    rdfs:label "Document"@en ;
    rdfs:comment "A written or electronic record."@en .

sei-doc:RegulatoryFiling rdf:type owl:Class ;
    rdfs:subClassOf sei-doc:Document ;
    rdfs:label "Regulatory Filing"@en ;
    rdfs:comment "A document filed with a regulatory body."@en .

sei-doc:SECFiling rdf:type owl:Class ;
