sei-doc:Form10K rdf:type owl:Class ;
    rdfs:subClassOf sei-doc:SECFiling ;
    rdfs:label "Form 10-K"@en ;
    rdfs:comment "Annual report filed by public companies."@en ;
    skos:definition "A comprehensive summary of a company's financial performance."@en .

sei-doc:Form10Q rdf:type owl:Class ;
    rdfs:subClassOf sei-doc:SECFiling ;
    rdfs:label "Form 10-Q"@en ;
    rdfs:comment "Quarterly report filed by public companies."@en .

sei-doc:Form8K rdf:type owl:Class ;
    rdfs:subClassOf sei-doc:SECFiling ;
    rdfs:label "Form 8-K"@en ;
    rdfs:comment "Current report for material events."@en .

sei-doc:ProxyStatement rdf:type owl:Class ;
    rdfs:subClassOf sei-doc:SECFiling ;
    rdfs:label "Proxy Statement"@en ;
    skos:altLabel "DEF 14A"@en .

sei-doc:EquityResearchReport rdf:type owl:Class ;
    rdfs:subClassOf sei-doc:Document ;
    rdfs:label "Equity Research Report"@en .

sei-doc:EarningsCallTranscript rdf:type owl:Class ;
    rdfs:subClassOf sei-doc:Document ;
    rdfs:label "Earnings Call Transcript"@en .

sei-doc:PressRelease rdf:type owl:Class ;
    rdfs:subClassOf sei-doc:Document ;
    rdfs:label "Press Release"@en .

# Document Sections (for 10-K)
sei-doc:DocumentSection rdf:type owl:Class ;
    rdfs:label "Document Section"@en .

sei-doc:BusinessDescription rdf:type owl:Class ;
    rdfs:subClassOf sei-doc:DocumentSection ;
    rdfs:label "Business Description"@en ;
    rdfs:comment "Item 1 of Form 10-K"@en .

sei-doc:RiskFactors rdf:type owl:Class ;
    rdfs:subClassOf sei-doc:DocumentSection ;
    rdfs:label "Risk Factors"@en ;
    rdfs:comment "Item 1A of Form 10-K"@en .

sei-doc:ManagementDiscussion rdf:type owl:Class ;
    rdfs:subClassOf sei-doc:DocumentSection ;
    rdfs:label "Management Discussion and Analysis"@en ;
    skos:altLabel "MD&A"@en ;
    rdfs:comment "Item 7 of Form 10-K"@en .

sei-doc:FinancialStatements rdf:type owl:Class ;
    rdfs:subClassOf sei-doc:DocumentSection ;
    rdfs:label "Financial Statements"@en ;
    rdfs:comment "Item 8 of Form 10-K"@en .

