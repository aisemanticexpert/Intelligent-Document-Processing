sei-risk:CybersecurityRisk rdf:type owl:Class ;
    rdfs:subClassOf sei-risk:OperationalRisk ;
    rdfs:label "Cybersecurity Risk"@en .

sei-risk:FinancialRisk rdf:type owl:Class ;
